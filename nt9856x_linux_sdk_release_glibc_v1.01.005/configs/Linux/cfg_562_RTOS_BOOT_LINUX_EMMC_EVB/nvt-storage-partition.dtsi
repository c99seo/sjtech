/*
 * Novatek Ltd. NA51089 BSP part of dts
 *
 * Cortex-A9
 *
 * !!!!!! WARNING !!!!!
 * !!!!!! for emmc,
 * !!!!!! 1. rootfs, rootfs1 and rootfs2, mbr1, rootfsl1, mbr2, rootfsl2, ... (rootfsl l=logical partition in fdisk)
 * !!!!!!    all must be inascending order and continuous in the bottom of emmc. (with naming rule)
 * !!!!!! 2. rootfs, rootfs1 and rootfs2 must be existing; mbr1, rootfsl1 are optional.
 */

&emmc {
	partition_mbr {         label = "mbr";                    reg = <0x0 0x00000000 0x0 0x00000200>; };  /* Fixed */
	partition_fdt {	        label = "fdt";	                  reg = <0x0 0x00040000 0x0 0x00040000>; };  /* Fixed */
	partition_fdt.restore {	label = "fdt.restore";            reg = <0x0 0x00080000 0x0 0x00040000>; };  /* Fixed */
	partition_uboot {       label = "uboot";                  reg = <0x0 0x000C0000 0x0 0x00200000>; };
	partition_uenv {        label = "uenv";                   reg = <0x0 0x002C0000 0x0 0x00040000>; };
	partition_kernel {      label = "kernel";                 reg = <0x0 0x00300000 0x0 0x00500000>; };
	partition_fdt.app {     label = "fdt.app";                reg = <0x0 0x00800000 0x0 0x00A00000>; };
	partition_rtos {        label = "rtos";               	  reg = <0x0 0x01200000 0x0 0x00500000>; };
	partition_rootfs {      label = "rootfs";  active="true"; reg = <0x0 0x01700000 0x0 0x1A000000>; };  /* root-fs (active="true" is to assign rootfs partition device) */
	partition_rootfs1 {     label = "rootfs1";                reg = <0x0 0x1B700000 0x0 0x0A000000>; };  /* cache ota partition */
	partition_rootfs2 {     label = "app";                    reg = <0x0 0x25700000 0x0 0x0C000000>; };  /* reserved for user e.g: rescue disk */
	partition_mbr1 {        label = "mbr1";                   reg = <0x0 0x31700000 0x0 0x00400000>; };
	partition_rootfsl1{     label = "rootfsl1";               reg = <0x0 0x31B00000 0x0 0x00000000>; };  /* optional (user defined), for last extend, size must be zero */
};
