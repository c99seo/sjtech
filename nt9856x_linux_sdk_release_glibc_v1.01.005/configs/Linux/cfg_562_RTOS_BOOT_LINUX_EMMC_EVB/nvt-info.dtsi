		EMBMEM_BLK_SIZE = "0x200";

		/**
		 * [EMBMEM]
		 * EMBMEM_NONE
		 * EMBMEM_NAND
		 * EMBMEM_SPI_NOR
		 * EMBMEM_SPI_NAND
		 * EMBMEM_EMMC
		 */
		EMBMEM = "EMBMEM_EMMC";

		/**
		 * [FW_TYPE]
		 * FW_TYPE_NORMAL
		 * FW_TYPE_COMPRESS
		 * FW_TYPE_PARTIAL
		 * FW_TYPE_PARTIAL_COMPRESS
		 * FW_TYPE_LZMA
		 */
		FW_TYPE = "FW_TYPE_PARTIAL_COMPRESS";

		/**
		 * [LCD1]
		 * disp_off
		 * disp_if8b_lcd1_wm08001_sn75lvds83b
		 * disp_if8b_lcd1_aucn01
		 * disp_ifdsi_lcd1_nt35510
		 * disp_if8b_lcd1_pw35p00
		 */
		LCD1 = "disp_if8b_lcd1_pw35p00";

		/**
		 * [SENSOR]
		 * sen_off
		 * sen_ar0237ir
		 * sen_os02k10
		 * sen_ov2715
		 * sen_ar0237
		 * sen_imx290
		 * sen_os05a10
		 * sen_f35
		 * sen_f37
		 * sen_gc4653
		 */
		SENSOR1 = "sen_imx290";
		SENSOR2 = "sen_off";
		SENSOR_CFG_SUFFIX = "_56x";

		/* disable unused rtos's package */
		PACKAGE_VIDEO    = "0";
		PACKAGE_AUDIO    = "0";
		PACKAGE_FILESYS  = "0";
		PACKAGE_SDCARD   = "0";
		PACKAGE_DISPLAY  = "0";
		PACKAGE_SAMPLES  = "0";
		PACKAGE_CMDSYS   = "0";

		/**
		* [BOOT_OS]
		* BOOT_OS_LINUX
		* BOOT_OS_RTOS
		* BOOT_OS_RTOS_BOOT_LINUX
		*/
		BOOT_OS = "BOOT_OS_RTOS_BOOT_LINUX";

		/**
		 * ======= Linux common =========
		 * application/external
		 */
		NVT_CFG_APP_EXTERNAL = "";
		/* application include list */
		NVT_CFG_APP = "mem isp_demon memcpy drystone-2.0";
		/* rootfs etc folder */
		NVT_ROOTFS_ETC = "RTOS_BOOT_LINUX_EVB";
		/* strip executable binary and library files: yes/no */
		NVT_BINARY_FILE_STRIP = "yes";
		/* Using customized kernel config */
		NVT_CFG_KERNEL_CFG = "na51089_evb_rtos_small_defconfig_emmc_release";
		/* run script befor making rootfs for 'make post' */
		NVT_MAKE_POST = "make_post.sh";
		/* disable make install on sample, hdal/samples, hdal/test_cases */
		NVT_SAMPLES_INSTALL = "DISABLE";
		/* Using customized uboot config */
		NVT_CFG_UBOOT_CFG = "";
		/* Optional: customized CONFIG_NVT_UIMAGE_SIZE and CONFIG_NVT_ALL_IN_ONE_IMG_SIZE */
		NVT_CFG_UBOOT_UIMAGE_SIZE = "0x400000";
		NVT_CFG_UBOOT_ALL_IN_ONE_IMG_SIZE = "0x1D00000";

		/**
		 * ======= Linux for different code setting =========
		 * [NVT_LINUX_SMP]
		 * NVT_LINUX_SMP_ON
		 * NVT_LINUX_SMP_OFF
		 */
		NVT_LINUX_SMP = "NVT_LINUX_SMP_OFF";

		/**
		 * [NVT_CHIP_ID]
		 * CHIP_NA51055
		 * CHIP_NA51084
		 * CHIP_NA51089
